#include <stdio.h>
int main()
{
  char temp;
  int amount;
  scanf("%d",&amount);
  int input[amount];
  int i=0;
  do {
    scanf("%d%c",&input[i],&temp);
    i++;
  } while(temp != '\n');
  int nub[amount];
  for(i=0;i<amount;i++)
{
	nub[i]=1;
}
  i=0;
  int j=0;
  for(j=1;j<amount;j++)
  {
  for(i=0;i<j;i++)
  {
  	if(input[j]>input[i])
  	{
  		if(nub[i]>=nub[j])
  	nub[j]=nub[i]+1;	
	}
  }
}
for(i=0;i<amount;i++)
{
	printf("%d ",nub[i]);
}

  return 0;
}
