#include <stdio.h>
#include <string.h>
#include <windows.h>


void Morse_Code(char Message[]);

int main()
{
	char Message_Input[100];
	
	//Input Message
	printf("Enter Message : ");
	gets(Message_Input);
	
	Morse_Code(Message_Input);
	
    return 0;
}

void Morse_Code(char Message[])
{
	int i;
		for(i=0;i<strlen(Message);i++)
	{
		if(Message[i]=='1')
		{printf("[1]");
		Beep(1000,200);
		printf(".");
		Beep(1200,200);
		printf("-");
		Beep(1200,200);
		printf("-");
		Beep(1200,200);
		printf("-");
